<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Test Symfony PA!{% endblock %}</title>
        {% block stylesheets %}
            <link rel="stylesheet" href="{{asset( 'assets/vendor/bootstrap/dist/css/bootstrap.min.css')}}" />
        {% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
    {% block body %}{% endblock %}
    {% block javascripts %}
        {% javascripts 'assets/vendor/*' output='js/compiled/main.js'  %}
        <script src="{{ asset_url }}"></script>
        {% endjavascripts %}
    {% endblock %}
</body>

</html>
